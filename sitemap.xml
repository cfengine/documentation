<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {% for page in site.pages %}
    {% if page.published == true %}
    {% if page.layout != nil %}
    {% if page.layout != 'feed' %}
    <url>
        <loc>{{site.baseurl}}{{ page.alias | remove: 'index.html' }}</loc>
    </url>
    {% endif %}
    {% endif %}
    {% endif %}
    {% endfor %}
</urlset>